<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <UseWpf>true</UseWpf>
    <TargetFrameworks>net5.0-windows</TargetFrameworks>
    <GenerateAssemblyInfo>true</GenerateAssemblyInfo>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
    <Description>UI library for minecraft login with Xbox account</Description>
    <Copyright>Copyright (c) 2021 AlphaBs</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/CmlLib/CmlLib.Core.Auth.Microsoft.UI</PackageProjectUrl>
    <RepositoryUrl>https://github.com/CmlLib/CmlLib.Core.Auth.Microsoft.UI</RepositoryUrl>
    <Authors>ksi123456ab</Authors>
    <PackageTags>CmlLib Minecraft Login Auth Authentication Microsoft Xbox Live XboxLive UI Wpf</PackageTags>
    <Version>1.1.0-beta2</Version>
    <PackageIcon>icon.png</PackageIcon>
    <RepositoryType>git</RepositoryType>
    <PackageId>CmlLib.Core.Auth.Microsoft.UI.Wpf</PackageId>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\CmlLib.Core.Auth.Microsoft\CmlLib.Core.Auth.Microsoft.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Include="../icon.png" Pack="true" Visible="false" PackagePath="" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Web.WebView2" Version="1.0.1210.39" />
  </ItemGroup>
</Project>